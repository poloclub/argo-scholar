          <attvalue for="4" value="2020-01-21" />
          <attvalue for="5" value="Xu, Xintian; Chen, Ping; Wang, Jingfang; Feng, Jiannan; Zhou, Hui; Li, Xuan; Zhong, Wu; Hao, Pei" />
          <attvalue for="6" value="Sci China Life Sci" />
